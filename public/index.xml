	width=&#34;382&#34;
	height=&#34;180&#34;
	srcset=&#34;//localhost:1313/p/coffee-sales/1_hu142118351375747506.JPG 480w, //localhost:1313/p/coffee-sales/1_hu14628962142816861244.JPG 1024w&#34;
	loading=&#34;lazy&#34;
	
